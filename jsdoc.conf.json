{
    "plugins": ["jsdoc_plugins/ext_xtype"],
    "templates": {
        "default": {
            "useLongnameInNav": true
        }
    },
    "recurseDepth": 6,
    "source": {
        "include": [
            "app.js",
            "app",
            "app/api",
            "app/helper",
            "app/mixin",
            "app/widget/",
            "lib",
            "overrides"
        ]
    },
    "tags": {
        "dictionaries": ["jsdoc", "closure"]
    },
    "opts": {
        "destination": "./doc",
        "recursive": true,
        "readme": "./Readme.md"
    }
}